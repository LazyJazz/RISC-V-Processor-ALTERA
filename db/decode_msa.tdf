--synthesis_resources = lut 4 
SUBDESIGN decode_msa
( 
	data[1..0]	:	input;
	enable	:	input;
	eq[3..0]	:	output;
) 
VARIABLE 
	data_wire[1..0]	: WIRE;
	enable_wire	: WIRE;
	eq_node[3..0]	: WIRE;
	eq_wire[3..0]	: WIRE;
	w_anode1091w[2..0]	: WIRE;
	w_anode1104w[2..0]	: WIRE;
	w_anode1112w[2..0]	: WIRE;
	w_anode1120w[2..0]	: WIRE;

BEGIN 
	data_wire[] = data[];
	enable_wire = enable;
	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode1120w[2..2], w_anode1112w[2..2], w_anode1104w[2..2], w_anode1091w[2..2]);
	w_anode1091w[] = ( (w_anode1091w[1..1] & (! data_wire[1..1])), (w_anode1091w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode1104w[] = ( (w_anode1104w[1..1] & (! data_wire[1..1])), (w_anode1104w[0..0] & data_wire[0..0]), enable_wire);
	w_anode1112w[] = ( (w_anode1112w[1..1] & data_wire[1..1]), (w_anode1112w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode1120w[] = ( (w_anode1120w[1..1] & data_wire[1..1]), (w_anode1120w[0..0] & data_wire[0..0]), enable_wire);
END;
